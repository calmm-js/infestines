{
  "name": "infestines",
  "version": "0.4.0",
  "description": "A minimalistic library to support writing FP libraries",
  "main": "lib/infestines.js",
  "scripts": {
    "bench": "NODE_ENV=production node --expose-gc bench/bench.js",
    "dist": "npm run dist:bowerify & npm run dist:bowerify.min & npm run dist:commonjs & wait",
    "dist:bowerify": "    NODE_ENV=dev        browserify src/infestines.js -t babelify --no-bundle-external -d -s I -g envify                               > dist/infestines.js",
    "dist:bowerify.min": "NODE_ENV=production browserify src/infestines.js -t babelify --no-bundle-external    -s I -g envify -g uglifyify | uglifyjs -m -c > dist/infestines.min.js",
    "dist:commonjs": "babel src --source-maps inline --out-dir lib",
    "docs": "./scripts/generate-docs",
    "docs-bundle": "NODE_ENV=dev browserify -r ./lib/infestines:infestines -r ramda -r babel-polyfill -g envify -g uglifyify | uglifyjs -m -c > docs/bundle.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run test && npm run dist",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polytypic/infestines.git"
  },
  "keywords": [
    "internal"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/polytypic/infestines/issues"
  },
  "homepage": "https://github.com/polytypic/infestines#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-check-es2015-constants": "^6.22.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.22.0",
    "babel-plugin-transform-es2015-computed-properties": "^6.22.0",
    "babel-plugin-transform-es2015-destructuring": "^6.22.0",
    "babel-plugin-transform-es2015-duplicate-keys": "^6.22.0",
    "babel-plugin-transform-es2015-for-of": "^6.22.0",
    "babel-plugin-transform-es2015-function-name": "^6.22.0",
    "babel-plugin-transform-es2015-literals": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-es2015-object-super": "^6.22.0",
    "babel-plugin-transform-es2015-parameters": "^6.22.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.22.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.22.0",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "babel-plugin-transform-es2015-unicode-regex": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-register": "^6.22.0",
    "babelify": "^7.3.0",
    "benchmark": "^2.1.3",
    "browserify": "^14.0.0",
    "codecov": "^1.0.1",
    "envify": "^4.0.0",
    "eslint": "^3.14.1",
    "marked": "^0.3.6",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "ramda": "^0.23.0",
    "sprintf-js": "^1.0.3",
    "uglify-js": "^2.7.5",
    "uglifyify": "^3.0.4"
  }
}
